{
  "expo": {
    "name": "CulturTap Genie",
    "slug": "genie-user",
    "version": "1.36.0",
    "orientation": "portrait",
    "icon": "./App/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./App/assets/Splashh.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(Genie) to use your location."
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "36"
    },
    "android": {
      "versionCode": 36,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "clipboard"
      ],
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./App/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.culturtapgenie.Genie"
    },
    "web": {
      "favicon": "./App/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8534107f-6fb3-4c37-869e-15b5d2071603"
      }
    },
    "owner": "culturtap-genie"
  }
}